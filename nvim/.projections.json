{
  "*.fnl": {
    "type": "module",
    "alternate": [
      "{}.lua"
    ]
  },
  "*.lua": {
    "alternate": [
      "{}.fnl"
    ]
  },
  "fnl/*.fnl": {
    "type": "module",
    "template": [
      "(local core (require :nfnl.core))"
    ],
    "alternate": [
      "lua/{}.lua"
    ]
  },
  "lua/*.lua": {
    "alternate": [
      "fnl/{}.fnl"
    ]
  },
  "fnl/own/plugin/*.fnl": {
    "type": "plugin",
    "template": [
      "(local core (require :nfnl.core))"
    ],
    "alternate": [
      "lua/own/plugin/{}.lua"
    ]
  },
  "lua/own/plugin/*.lua": {
    "alternate": [
      "fnl/own/plugin/{}.fnl"
    ]
  }
}
