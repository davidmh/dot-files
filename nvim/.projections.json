{
  "*.fnl": {
    "alternate": [
      "{}.lua"
    ]
  },
  "*.lua": {
    "alternate": [
      "{}.fnl"
    ]
  },
  "fnl/own/*.fnl": {
    "type": "module",
    "template": [
      "(local core (require :nfnl.core))"
    ],
    "alternate": [
      "lua/own/{}.lua"
    ]
  },
  "lua/own/*.lua": {
    "alternate": [
      "fnl/own/{}.fnl"
    ]
  },
  "fnl/own/plugin/*.fnl": {
    "type": "plugin",
    "template": [
      "(import-macros {: map} :own.macros)",
      "(local {: autoload} (require :nfnl.module))",
      "(local core (autoload :nfnl.core))"
    ],
    "alternate": [
      "lua/own/plugin/{}.lua"
    ]
  },
  "lua/own/plugin/*.lua": {
    "alternate": [
      "fnl/own/plugin/{}.fnl"
    ]
  }
}
